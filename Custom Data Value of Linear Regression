import numpy as np
import numpy as np
import pandas as pd
import matplotlib.pyplot as plt
from sklearn.linear_model import LinearRegression

cars = pd.read_csv('cardata.csv')
print(cars.head())
print(cars.columns)

plt. figure(figsize=(16,8))
plt. scatter(
    cars['Car_Name'],
    cars['Selling_Price'],
    color='blue'
)

plt.xlabel=("Car_Name")
plt.ylabel=("Selling_Price")
plt.show()
